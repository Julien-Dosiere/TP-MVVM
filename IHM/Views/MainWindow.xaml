        </StackPanel>
        <Frame x:Name="frmPages" NavigationUIVisibility="Hidden" Content="Page Defaut" HorizontalAlignment="Left"  Height="420" Margin="138,0,-0.4,0" VerticalAlignment="Top" Width="856" Background="#FF494949"/>

