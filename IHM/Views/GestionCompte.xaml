<Page x:Class="IHM.Views.GestionCompte"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IHM.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GestionCompte">

    <Grid Background="#FFEBE9D3">
        <TextBox x:Name="tbxNom" HorizontalAlignment="Left" Height="23" Margin="375,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="161"/>
        <TextBox x:Name="tbxPass" HorizontalAlignment="Left" Height="23" Margin="375,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="161"/>
        <Label Content="Nom de Compte" HorizontalAlignment="Left" Margin="240,133,0,0" VerticalAlignment="Top" Height="38" Width="94" FontSize="14"/>
        <Label Content="Mot de Passe" HorizontalAlignment="Left" Margin="240,176,0,0" VerticalAlignment="Top" Height="33" Width="94" FontSize="14"/>
        <Button Content="Enregistrer" HorizontalAlignment="Left" Margin="375,263,0,0" VerticalAlignment="Top" Width="161" FontSize="14" Height="31" Click="Enregistrer_Click"/>

    </Grid>
</Page>
